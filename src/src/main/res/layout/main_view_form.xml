<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_marginBottom="11dip"
    android:layout_marginTop="11dip"
    android:layout_marginLeft="11dip"
    android:layout_marginRight="11dip"
    android:layout_height="match_parent">


    <include
        android:layout_width="match_parent"
        android:layout_height="44dip"
        layout="@layout/main_view_cell_space"
        android:layout_gravity="right" />


    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/main_view_cell_country"
        android:layout_gravity="center_horizontal" />

    <View
        android:layout_gravity="bottom"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#999999"/>

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/main_view_cell_latitud"
        android:layout_gravity="center_horizontal" />

    <View
        android:layout_gravity="bottom"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#999999"/>

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/main_view_cell_longitud"
        android:layout_gravity="right" />

    <View
        android:layout_gravity="bottom"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#999999"/>

    <include
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        layout="@layout/main_view_cell_cost"
        android:layout_gravity="right" />

    <View
        android:layout_gravity="bottom"
        android:layout_width="fill_parent"
        android:layout_height="1dip"
        android:background="#999999"/>

    <include
        android:layout_width="match_parent"
        android:layout_height="44dip"
        layout="@layout/main_view_cell_space"
        android:layout_gravity="right" />


</LinearLayout>